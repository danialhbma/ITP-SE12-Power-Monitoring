        "targets": [
          {
            "datasource": {
              "type": "influxdb",
              "uid": "d4311848-4e92-43a6-a074-002e5b15cec7"
            },
            "query": "f1 = \r\n  from(bucket: \"CO2\")\r\n    |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\r\n    |> filter(fn: (r) => r[\"_measurement\"] == \"rack1_co2\")\r\n    |> aggregateWindow(every: 30m, fn: mean, createEmpty: false)\r\n\r\nf2 = \r\n  from(bucket: \"Humidity\")\r\n    |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\r\n    |> filter(fn: (r) => r[\"_measurement\"] == \"rack1_humidity\")\r\n    |> aggregateWindow(every: 30m, fn: mean, createEmpty: false)\r\n    |> map(fn: (r) => ({ r with _value: float(v: r._value) * 10.0 }))\r\n\r\nf3 = \r\n  from(bucket: \"Temperature\")\r\n    |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\r\n    |> filter(fn: (r) => r[\"_measurement\"] == \"rack1_temperature\")\r\n    |> aggregateWindow(every: 30m, fn: mean, createEmpty: false)\r\n    |> map(fn: (r) => ({ r with _value: float(v: r._value) * 20.0 }))\r\n\r\nf4 = \r\n  from(bucket: \"Light\")\r\n    |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\r\n    |> filter(fn: (r) => r[\"_measurement\"] == \"rack1_light\")\r\n    |> aggregateWindow(every: 30m, fn: mean, createEmpty: false)\r\n\r\nf5 = \r\n  from(bucket: \"Water\")\r\n    |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\r\n    |> filter(fn: (r) => r[\"_measurement\"] == \"rack1_water\")\r\n    |> aggregateWindow(every: 30m, fn: mean, createEmpty: false)\r\n\r\nf6 = \r\n  from(bucket: \"Power Consumption\")\r\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\r\n  |> filter(fn: (r) => r[\"_measurement\"] == \"Rack_1_Light\" or r[\"_measurement\"] == \"Rack_1_Water\")\r\n  |> aggregateWindow(every: 30m, fn: mean, createEmpty: false)\r\n  |> map(fn: (r) => ({\r\n      r with\r\n      _value: if r[\"_measurement\"] == \"Rack_1_Water\" then r._value * 20.0 else r._value\r\n    }))\r\n\r\nunion(tables: [f1, f2, f3, f4, f5, f6])",
            "refId": "A"
          }
        ],
        "title": "Rack 1 Stats",
        "type": "timeseries"
      },
